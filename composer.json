{
    "name": "loot/spinoza",
    "description": "Package can generate documentation for service",
    "license": "MIT",
    "type": "psalm-plugin",
    "version": "0.1",
    "authors": [
        {
            "name": "Loot",
            "email": "77028277827@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Loot\\Spinoza\\": "src"
        }
    },
    "require": {
        "loot/phpdoc-reader": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Loot\\Spinoza\\SpinozaServiceProvider"
            ]
        },
        "psalm": {
            "pluginClass": "Loot\\Spinoza\\Plugin"
        }
    }
}
